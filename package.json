  "scripts": {
    "build": "cross-env CI=false NODE_ENV=production remix vite:build",
    "build:ci": "cross-env CI=false NODE_ENV=production remix vite:build",
    "dev": "node ./scripts/dev.cjs && remix vite:dev",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "vercel-build": "node ./scripts/vercel-build.js",
    "vercel-direct-build": "cross-env CI=false NODE_ENV=production remix vite:build",
    "vercel-fallback-build": "node ./scripts/vercel-fallback-build.js",
    "postinstall": "node ./scripts/draco.cjs",
    "deploy": "npm run build && wrangler pages deploy ./build/client --project-name portfolio",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "deploy:storybook": "wrangler pages deploy ./storybook-static --project-name portfolio-storybook --branch main",
    "cleanup-deps": "node ./scripts/cleanup-deps.cjs",
    "generate:patch": "node ./scripts/create-patches.js",
    "cloudflare-build": "node ./scripts/cloudflare-build.cjs",
    "cloudflare-direct-build": "node ./scripts/cloudflare-direct-build.js",
    "cloudflare-pages-build": "node ./scripts/cloudflare-pages-build.js",
    "fix-fs-extra": "node ./scripts/fix-fs-extra.cjs",
    "fix-package-lock": "node ./scripts/package-lock-fix.js"
  },
